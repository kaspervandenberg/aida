<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:pub="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Proto-ontology/Publication.owl#"
    xmlns="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Enriched-ontology/DiscoveredEntities.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:owl11="http://www.w3.org/2006/12/owl11#"
    xmlns:owl11xml="http://www.w3.org/2006/12/owl11-xml#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Enriched-ontology/DiscoveredEntities.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Proto-ontology/Publication.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="RelationShipDiscoveredEntity">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Entity that was discoverd in text by discovery of a semantic relationship with another entity, e.g. PKB/Akt in 'PKB/Akt Phosphorylates p27'</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="#RelationShipDiscoveredEntity"/>
            <owl:onProperty rdf:resource="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Proto-ontology/Publication.owl#semantic_relationship_with"/>
          </owl:Restriction>
          <owl:Class rdf:ID="TextMiningDiscoveredEntity"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >related entity</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="TextMiningProcedure">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Procedure"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >text mining procedure</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Text mining procedure</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="HumanCurationProcedure">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Procedure"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >human curation</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Procedure involving evaluation by human expert</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="EntityDiscoveryProcedure">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Procedure by which an entity was discovered</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="discovered_entity"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:ID="DiscoveredEntity"/>
            </owl:someValuesFrom>
          </owl:Restriction>
          <owl:Class rdf:about="#Procedure"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >procedure</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Procedure">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Procedure</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >discovery procedure</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Workflow">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Procedure"/>
          <owl:Restriction>
            <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:minCardinality>
            <owl:onProperty>
              <owl:DatatypeProperty rdf:ID="workflow_reference"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="HumanCuratedDiscoveredEntity">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#DiscoveredEntity"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#HumanCurationProcedure"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="discoveredThroughProcedure"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="HumanCuratedTextMiningProcedure">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Text mining procedure involving evaluation by human expert</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#HumanCurationProcedure"/>
          <owl:Class rdf:about="#TextMiningProcedure"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >human curated text mining</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#DiscoveredEntity">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Procedure"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#discoveredThroughProcedure"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="ColocationDiscoveredEntity">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >colocated entity</rdfs:label>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#TextMiningDiscoveredEntity"/>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="#ColocationDiscoveredEntity"/>
            <owl:onProperty rdf:resource="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Proto-ontology/Publication.owl#colocated_with"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Entity discovered in one unit of text by colocation with another entity</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="RetrievedDocument">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#TextMiningDiscoveredEntity"/>
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:ID="DocumentRetrievalProcedure"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#discoveredThroughProcedure"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#TextMiningDiscoveredEntity">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#discoveredThroughProcedure"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="TextMiningWorkflow"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="#DiscoveredEntity"/>
  </owl:Class>
  <owl:Class rdf:about="#TextMiningWorkflow">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >text mining workflow</rdfs:label>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#TextMiningProcedure"/>
          <owl:Class rdf:about="#Workflow"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Discovery through a text mining workflow (I expect the class ID to be the URI of the workflow)</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#DocumentRetrievalProcedure">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#TextMiningProcedure"/>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Proto-ontology/Publication.owl#DocumentSearch"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="uses_document_search"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="discoveredIn">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Wherein a discovery was made (if anything)</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >discovered in</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#discovered_entity">
    <owl:inverseOf>
      <owl:FunctionalProperty rdf:about="#discoveredThroughProcedure"/>
    </owl:inverseOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >discovered</rdfs:label>
    <rdfs:domain rdf:resource="#Procedure"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >procedure through which an entity was discovered</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#uses_document_search">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >something defined by a search</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >uses</rdfs:label>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="used_to_retrieve_documents_by"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="#DocumentRetrievalProcedure"/>
    <rdfs:range rdf:resource="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Proto-ontology/Publication.owl#DocumentSearch"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#used_to_retrieve_documents_by">
    <rdfs:range rdf:resource="#DocumentRetrievalProcedure"/>
    <rdfs:domain rdf:resource="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Proto-ontology/Publication.owl#DocumentSearch"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >some search defines something</rdfs:comment>
    <owl:inverseOf rdf:resource="#uses_document_search"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >used by</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="hasScore">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Score (for ranking) of an entity according to some metric</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >has score</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#workflow_reference">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >workflow reference</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >any reference to a workflow</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="DiscoveredEntities_DatatypeProperty_2"/>
  <owl:DatatypeProperty rdf:ID="hasLikelihoodScore">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >has likelihood score</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Score according to the likelihood formula: -log ( (a/b) / (c/d) )</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:subPropertyOf rdf:resource="#hasScore"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="workflow_url">
    <rdfs:subPropertyOf rdf:resource="#workflow_reference"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >workflow url</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >url of workflow (e.g. on myExperiment.org)</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:about="#discoveredThroughProcedure">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >discovery status of an entity (how it was discovered)</rdfs:comment>
    <owl:inverseOf rdf:resource="#discovered_entity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Procedure"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="workflow_name">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >name</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >name of workflow</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#workflow_reference"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <TextMiningWorkflow rdf:ID="ExampleInstance_TextMiningDiscoveryWorkflow">
    <workflow_reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >example workflow reference</workflow_reference>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Example instance of Text Mining Discovery Worfklow</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Example instance Text Mining Discovery Workflow</rdfs:label>
  </TextMiningWorkflow>
</rdf:RDF>

