  <owl:Class rdf:ID="DNA_RNA_status">
    <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/basic-bio/refining-entities-and-properties.owl#Intrinsic_characteristic"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="DNA_nucleotide_status"/>
          <owl:Class rdf:ID="RNA_nucleotide_status"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="DNA_molecule_part">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.co-ode.org/ontologies/basic-bio/advanced-relational-properties.owl#has_determinate_part"/>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="DNA_nucleotide"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.co-ode.org/ontologies/basic-bio/advanced-relational-properties.owl#has_determinate_part"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#DNA_nucleotide"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/HistOn2/top-bio-extended.owl#Biological_macromolecule_part"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >DNA molecule that is part of a real or virtual DNA molecule.
Not restricted to part_of DNA molecule, because it is allowed to exist on its own.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#RNA_nucleotide_status">
