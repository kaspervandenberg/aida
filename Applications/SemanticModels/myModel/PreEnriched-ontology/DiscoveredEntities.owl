<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Enriched-ontology/DiscoveredEntities.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:owl11="http://www.w3.org/2006/12/owl11#"
    xmlns:owl11xml="http://www.w3.org/2006/12/owl11-xml#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://rdf.adaptivedisclosure.org/owl/BioAID/myModel/Enriched-ontology/DiscoveredEntities.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="HumanCuratedDiscoveredEntity">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DiscoveredEntity"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="HumanCuratedDiscoveryProcedure"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="discoveredThroughProcedure"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#HumanCuratedDiscoveryProcedure">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Discovery evaluated by human expert</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="EntityDiscoveryProcedure"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#DiscoveredEntity">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#discoveredThroughProcedure"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#EntityDiscoveryProcedure"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#EntityDiscoveryProcedure">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >discovery procedure</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Procedure by which an entity was discovered</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="TextMiningDiscoveryWorkflow">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Discovery through a text mining workflow (I expect the class ID to be the URI of the workflow)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#EntityDiscoveryProcedure"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >workflow URI</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="TextMiningDiscoveredEntity">
    <rdfs:subClassOf rdf:resource="#DiscoveredEntity"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#discoveredThroughProcedure"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#TextMiningDiscoveryWorkflow"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="HumanCuratedTextMiningDiscoveryWorkflow">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#TextMiningDiscoveryWorkflow"/>
          <owl:Class rdf:about="#HumanCuratedDiscoveryProcedure"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="discoveredIn">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Wherein a discovery was made (if anything)</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >discovered in</rdfs:label>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="#discoveredThroughProcedure">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >discovery status of an entity (how it was discovered)</rdfs:comment>
    <rdfs:range rdf:resource="#EntityDiscoveryProcedure"/>
  </owl:FunctionalProperty>
</rdf:RDF>

