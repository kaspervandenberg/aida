<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://integrativebioinformatics.nl/Ontology/ProtoOntology/EnzymeDisease.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://integrativebioinformatics.nl/Ontology/ProtoOntology/EnzymeDisease.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="Protein">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Protein</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Disease">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Disease</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Enzyme">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Enzyme</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Protein"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Protein with catalytic properties</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="associatedWithEnzyme">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >associated with</rdfs:label>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="associatedWithDisease"/>
    </owl:inverseOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Permissive form of association: direct or indirect, thus transitive.</rdfs:comment>
    <rdfs:domain rdf:resource="#Disease"/>
    <rdfs:range rdf:resource="#Enzyme"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#associatedWithDisease">
    <owl:inverseOf rdf:resource="#associatedWithEnzyme"/>
    <rdfs:domain rdf:resource="#Enzyme"/>
    <rdfs:range rdf:resource="#Disease"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >associated with</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Permissive form of association: direct or indirect, thus transitive.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:AnnotationProperty rdf:ID="abbreviation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:AnnotationProperty>
  <Disease rdf:ID="Barretts_esophagus">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barretts esophagus</rdfs:label>
    <associatedWithEnzyme>
      <Enzyme rdf:ID="EZH2">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Enhancer of Zeste (EZH2) is one of two mammalian homologues of Drosophila E(Z), a SET domain protein that methylates K27H3 [23]. E(Z) is a Polycomb group (PcG) protein involved in homeotic gene repression in Drosophila [24]. (From: PMID: 17306846 [PubMed - as supplied by publisher])</rdfs:comment>
        <associatedWithDisease>
          <Disease rdf:ID="Cell_cycle_defects">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Cell cycle defects</rdfs:label>
            <associatedWithEnzyme rdf:resource="#EZH2"/>
          </Disease>
        </associatedWithDisease>
        <associatedWithDisease>
          <Disease rdf:ID="Wilms_tumor">
            <associatedWithEnzyme rdf:resource="#EZH2"/>
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Wilms tumor</rdfs:label>
          </Disease>
        </associatedWithDisease>
        <associatedWithDisease>
          <Disease rdf:ID="Colorectal_cancer">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Colorectal cancer</rdfs:label>
            <associatedWithEnzyme rdf:resource="#EZH2"/>
          </Disease>
        </associatedWithDisease>
        <associatedWithDisease>
          <Disease rdf:ID="Myeloma_Lymphoma">
            <associatedWithEnzyme rdf:resource="#EZH2"/>
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Myeloma lymphoma</rdfs:label>
          </Disease>
        </associatedWithDisease>
        <associatedWithDisease>
          <Disease rdf:ID="Breast_cancer">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Breast cancer</rdfs:label>
            <associatedWithEnzyme rdf:resource="#EZH2"/>
          </Disease>
        </associatedWithDisease>
        <associatedWithDisease>
          <Disease rdf:ID="Hepatocellular_carcinoma">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Hepatocellular carcinoma</rdfs:label>
            <associatedWithEnzyme rdf:resource="#EZH2"/>
          </Disease>
        </associatedWithDisease>
        <associatedWithDisease rdf:resource="#Barretts_esophagus"/>
        <associatedWithDisease>
          <Disease rdf:ID="Prostate_cancer">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Prostate cancer</rdfs:label>
            <associatedWithEnzyme rdf:resource="#EZH2"/>
          </Disease>
        </associatedWithDisease>
        <associatedWithDisease>
          <Disease rdf:ID="Melanoma">
            <associatedWithEnzyme rdf:resource="#EZH2"/>
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Melanoma</rdfs:label>
          </Disease>
        </associatedWithDisease>
        <associatedWithDisease>
          <Disease rdf:ID="Bladder_cancer">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Bladder cancer</rdfs:label>
            <associatedWithEnzyme rdf:resource="#EZH2"/>
          </Disease>
        </associatedWithDisease>
      </Enzyme>
    </associatedWithEnzyme>
  </Disease>
  <owl:AnnotationProperty rdf:ID="full_name">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:AnnotationProperty>
</rdf:RDF>

