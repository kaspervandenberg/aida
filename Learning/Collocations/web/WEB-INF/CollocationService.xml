<Context path="/CollocationService">
  <Logger className="org.apache.catalina.logger.FileLogger" prefix="CollocationService." suffix=".log" timestamp="true"/>
    <Valve className="org.apache.catalina.valves.AccessLogValve"
	    prefix="CollocationService_access_log." suffix=".txt"
		pattern="%t %a %m %U%q %{query}s %T"/>
    <Valve className="org.apache.catalina.valves.AccessLogValve"
	    prefix="CollocationService_query_log." suffix=".txt"
		pattern="%t %a %U %{query}s"/>
									   
</Context>
