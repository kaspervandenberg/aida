<Context path="/TestModel">
  <Logger className="org.apache.catalina.logger.FileLogger" prefix="TestModel." suffix=".log" timestamp="true"/>
    <Valve className="org.apache.catalina.valves.AccessLogValve"
	    prefix="TestModel_access_log." suffix=".txt"
		pattern="%t %a %m %U%q %{query}s %T"/>
    <Valve className="org.apache.catalina.valves.AccessLogValve"
	    prefix="TestModel_query_log." suffix=".txt"
		pattern="%t %a %U %{query}s"/>
									   
</Context>
