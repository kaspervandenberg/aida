<xsd:schema xmlns="http://aid.vle.org"             xmlns:xsd="http://www.w3.org/2001/XMLSchema"             xmlns:aid="http://aid.vle.org"             targetNamespace="http://aid.vle.org">  <xsd:element name="result" type="aid:resultType"/>    <xsd:complexType name="resultType">        <xsd:sequence>            <xsd:element name="doc"  type="document"  minOccurs="0" maxOccurs="unbounded"/>        </xsd:sequence>        <xsd:attribute name="query" type="xsd:string"/>        <xsd:attribute name="total" type="xsd:int"/>        <xsd:attribute name="time" type="xsd:decimal"/>            </xsd:complexType>    <xsd:complexType name="document">        <xsd:sequence>            <xsd:element name="field"  type="fieldType"  minOccurs="0" maxOccurs="unbounded"/>        </xsd:sequence>        <xsd:attribute name="score" type="xsd:decimal"/>        <xsd:attribute name="rank" type="xsd:int"/>    </xsd:complexType>    <xsd:complexType name="fieldType">        <xsd:all>            <xsd:element name="value"  type="xsd:string"/>        </xsd:all>        <xsd:attribute name="name" type="xsd:string" use="required"/>  </xsd:complexType>  </xsd:schema>